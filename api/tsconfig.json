{
    "compilerOptions": {
      "target": "ES2021",
      "module": "ESNext",
      "skipLibCheck": true,

      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
  
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noFallthroughCasesInSwitch": true
    }
  }